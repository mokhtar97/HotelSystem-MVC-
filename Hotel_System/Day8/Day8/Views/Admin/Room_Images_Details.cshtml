    Layout = "~/Views/Shared/_LayoutAdminpanel.cshtml";
}

<h2>Room_Images_Details</h2>

<div>
    <h4>Room_Images</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.roomType)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.roomType.Name)
        </dd>

